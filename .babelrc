{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "transform-class",
    "transform-flow-comments",
    "@babel/plugin-syntax-flow",
    "transform-flow-strip-types",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
